<Window x:Class="WPF_Lab_1._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Lab_1._1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="Set_Name" Content="Set Name" HorizontalAlignment="Left" Margin="52,76,0,0" VerticalAlignment="Top" Click="Set_Name_Click" Height="70" Width="112"/>
        <Button x:Name="Ret_Name" Content="Ret Name" HorizontalAlignment="Left" Margin="52,151,0,0" VerticalAlignment="Top" Click="Ret_Name_Click" Height="66" Width="112"/>
        <Label x:Name="label" Content="" HorizontalAlignment="Left" Margin="201,153,0,0" VerticalAlignment="Top" Width="199"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="201,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="199" Height="33" TextChanged="textBox_TextChanged"/>
        <Button Canvas.Left="10" Canvas.Top="10" Content="Click Me" Height="23" Name="button3" Width="75" Click="button3_Click"/>
        <TextBox x:Name="myTextBox" Canvas.Left="90" Canvas.Top="12" Width="170"/>
    </Canvas>
</Window>
